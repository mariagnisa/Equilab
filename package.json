{
  "name": "equilab",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "glamor": "^2.20.40",
    "node-sass-chokidar": "0.0.3",
    "react": "^15.6.1",
    "react-body-classname": "^1.2.0",
    "react-css-transition": "^0.7.4",
    "react-dom": "^15.6.1",
    "react-responsive-picture": "^2.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13",
    "react-simple-chatbot": "^0.2.7",
    "react-transition-group": "^1.2.1",
    "string-similarity": "^1.2.0",
    "styled-components": "^2.2.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/style/ -o src/style/",
    "watch-css": "npm run build-css && node-sass-chokidar src/style/ -o src/style/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.1"
  }
}
